{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "css",
  "testLibrary": "None",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/TemplateName.tsx",
        "style": "templates/TemplateName.module.css"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "page": {
      "customTemplates": {
        "component": "templates/TemplateName.tsx",
        "style": "templates/TemplateName.module.css"
      },
      "path": "src/components/pages",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "shared": {
      "customTemplates": {
        "component": "templates/TemplateName.tsx",
        "style": "templates/TemplateName.module.css"
      },
      "path": "src/components/shared",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    }
  }
}